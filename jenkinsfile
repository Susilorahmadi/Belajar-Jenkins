pipeline {
    agent any
    stages {
        stage('http') {
            steps {
                echo "using httpRequest"'
                httpRequest responseHandle: 'NONE', url: 'https://reqres.in/api/users', wrapAsMultipart: false
                echo "using curl"'
                bat curl https://reqres.in/api/users'
            }
        }
    }
}
